{
  "name": "AI Devtools 助手",
  "description": "使用 AI 分析和处理浏览器控制台信息",
  "version": "1.0",
  "manifest_version": 3,
  "devtools_page": "devtools.html",
  "permissions":["scripting", "tabs", "activeTab", "storage", "debugger"],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "host_permissions": [
    "*://*/*",
    "<all_urls>" 
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
} 